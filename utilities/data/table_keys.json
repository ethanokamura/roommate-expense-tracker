{
  "users": {
    "primary": ["user_id"],
    "foreign": [],
    "not_null": ["email"],
    "unique": ["email"]
  },
  "houses": {
    "primary": ["house_id"],
    "foreign": [],
    "not_null": ["name", "invite_code"],
    "unique": ["invite_code"]
  },
  "house_members": {
    "primary": ["house_member_id"],
    "foreign": ["user_id", "house_id"],
    "not_null": ["user_id", "house_id", "is_admin", "is_active"],
    "unique": ["user_id", "house_id"]
  },
  "expenses": {
    "primary": ["expense_id"],
    "foreign": ["house_member_id", "house_id"],
    "not_null": [
      "house_id",
      "house_member_id",
      "description",
      "total_amount",
      "is_settled"
    ],
    "unique": []
  },
  "expense_spits": {
    "primary": ["expense_split_id"],
    "foreign": [
      "expense_id",
      "house_member_id"
    ],
    "not_null": [
      "expense_id",
      "house_member_id",
      "amount_owed",
      "is_paid"
    ],
    "unique": []
  },
  "recurring_expenses": {
    "primary": ["recurring_expense_id"],
    "foreign": [
      "house_id",
      "house_member_id"
    ],
    "not_null": [
      "house_id",
      "house_member_id",
      "description_template",
      "amount_template",
      "frequency",
      "is_active"
    ],
    "unique": []
  },
  "receipts": {
    "primary": [
      "receipt_id"
    ],
    "foreign": [
      "expense_id",
      "user_id"
    ],
    "not_null": [
      "expense_id",
      "image_url",
      "user_id"
    ],
    "unique": [
      "expense_id"
    ]
  }
}
